html
  head 
    meta(lang="en" dir="ltr")
    title 
    link(rel="stylesheet" href="/static/css/main.css")
  
  body
    div(class="container")
      div(id="map-container" class="full-h container center")
        div(id="button-container" class="container-bar left-al abs bottom right")
